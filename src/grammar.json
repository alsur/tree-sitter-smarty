{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "smarty",
  "rules": {
    "template": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_smarty"
      }
    },
    "_smarty": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "inline"
        },
        {
          "type": "SYMBOL",
          "name": "include"
        },
        {
          "type": "SYMBOL",
          "name": "block"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "foreach"
        },
        {
          "type": "SYMBOL",
          "name": "for"
        },
        {
          "type": "SYMBOL",
          "name": "while"
        },
        {
          "type": "SYMBOL",
          "name": "if"
        },
        {
          "type": "SYMBOL",
          "name": "nocache"
        },
        {
          "type": "SYMBOL",
          "name": "literal"
        },
        {
          "type": "SYMBOL",
          "name": "function"
        },
        {
          "type": "SYMBOL",
          "name": "call"
        },
        {
          "type": "SYMBOL",
          "name": "capture"
        }
      ]
    },
    "_nested": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "inline"
        },
        {
          "type": "SYMBOL",
          "name": "include"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "foreach"
        },
        {
          "type": "SYMBOL",
          "name": "for"
        },
        {
          "type": "SYMBOL",
          "name": "while"
        },
        {
          "type": "SYMBOL",
          "name": "if"
        },
        {
          "type": "SYMBOL",
          "name": "nocache"
        },
        {
          "type": "SYMBOL",
          "name": "function"
        },
        {
          "type": "SYMBOL",
          "name": "capture"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+"
        },
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "PATTERN",
          "value": "[^*]"
        },
        {
          "type": "STRING",
          "value": "*"
        },
        {
          "type": "PATTERN",
          "value": "\\}+"
        }
      ]
    },
    "inline": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          },
          "named": true,
          "value": "php"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "SYMBOL",
                "name": "modifier"
              }
            ]
          }
        },
        {
          "type": "PATTERN",
          "value": "\\}+"
        }
      ]
    },
    "include": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+\\s*include"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "parameter"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\}+"
        }
      ]
    },
    "block": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+\\s*block"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "parameter"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\}+"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_nested"
            }
          },
          "named": true,
          "value": "body"
        },
        {
          "type": "PATTERN",
          "value": "\\{+\\/block\\}+"
        }
      ]
    },
    "foreach": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+\\s*foreach\\s+"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "PATTERN",
                  "value": "\\$\\w+"
                },
                {
                  "type": "PATTERN",
                  "value": "\\s+as\\s+"
                },
                {
                  "type": "PATTERN",
                  "value": "\\$\\w+"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "SEQ",
                      "members": [
                        {
                          "type": "PATTERN",
                          "value": "\\s+=>\\s+"
                        },
                        {
                          "type": "PATTERN",
                          "value": "\\$\\w+"
                        }
                      ]
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "parameter"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "PATTERN",
                        "value": "\\s+"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "parameter"
                      }
                    ]
                  }
                }
              ]
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\s*\\}+"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_nested"
              }
            },
            "named": true,
            "value": "body"
          }
        },
        {
          "type": "FIELD",
          "name": "alternative",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "foreach_else"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "PATTERN",
          "value": "\\{+\\/foreach\\}+"
        }
      ]
    },
    "foreach_else": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+\\s*foreachelse\\}+"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_nested"
            }
          },
          "named": true,
          "value": "body"
        }
      ]
    },
    "for": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+\\s*for"
        },
        {
          "type": "PATTERN",
          "value": "\\$[^\\s=]+\\s*=\\s*\\d+"
        },
        {
          "type": "STRING",
          "value": "to"
        },
        {
          "type": "PATTERN",
          "value": "\\d+"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "step"
                },
                {
                  "type": "PATTERN",
                  "value": "\\d+"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\}+"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_nested"
              }
            },
            "named": true,
            "value": "body"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\{+\\/for\\}+"
        }
      ]
    },
    "while": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+\\s*while"
        },
        {
          "type": "FIELD",
          "name": "condition",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[^\\}]+"
            },
            "named": true,
            "value": "text"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\}+"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_nested"
              }
            },
            "named": true,
            "value": "body"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\{+\\/while\\}+"
        }
      ]
    },
    "if": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+\\s*if"
        },
        {
          "type": "FIELD",
          "name": "condition",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[^\\}]+"
            },
            "named": true,
            "value": "text"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\}+"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_nested"
              }
            },
            "named": true,
            "value": "body"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "alternative",
            "content": {
              "type": "SYMBOL",
              "name": "else_if"
            }
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "alternative",
              "content": {
                "type": "SYMBOL",
                "name": "else"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "\\{+\\/if\\}+"
        }
      ]
    },
    "else_if": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+\\s*elseif"
        },
        {
          "type": "FIELD",
          "name": "condition",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[^\\}]+"
            },
            "named": true,
            "value": "text"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\}+"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_nested"
              }
            },
            "named": true,
            "value": "body"
          }
        }
      ]
    },
    "else": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+\\s*else\\}+"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_nested"
              }
            },
            "named": true,
            "value": "body"
          }
        }
      ]
    },
    "nocache": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+\\s*nocache\\}+"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_nested"
              }
            },
            "named": true,
            "value": "body"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\{+\\/nocache\\}+"
        }
      ]
    },
    "literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+\\s*literal\\}+"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_smarty"
              }
            },
            "named": true,
            "value": "text"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\{+\\/literal\\}+"
        }
      ]
    },
    "function": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+\\s*function\\s+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "parameter"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\}+"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_nested"
              }
            },
            "named": true,
            "value": "body"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\{+\\/function\\}+"
        }
      ]
    },
    "call": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+\\s*call\\s+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "parameter"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\}+"
        }
      ]
    },
    "capture": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{+\\s*capture\\s+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "parameter"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\}+"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_nested"
              }
            },
            "named": true,
            "value": "body"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\{+\\/capture\\}+"
        }
      ]
    },
    "modifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^|:}]+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ":"
              },
              {
                "type": "ALIAS",
                "content": {
                  "type": "PATTERN",
                  "value": "[^|:}]+"
                },
                "named": true,
                "value": "parameter"
              }
            ]
          }
        }
      ]
    },
    "parameter": {
      "type": "PATTERN",
      "value": "[^\\s=]+[\\s]*=[\\s]*('[^']*'|\"[^\"]*\"|\\[[^]]*]|[\\$][^\\s]*|[^\\s}]+)"
    },
    "text": {
      "type": "PREC",
      "value": -1,
      "content": {
        "type": "PATTERN",
        "value": "[^\\s\\|{*}-]([^\\|{*}]*[^\\|{*}-])?"
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s+"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}